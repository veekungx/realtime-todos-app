{
    "extends": "airbnb",
    "plugins": [
        "jest"
    ],
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "no-underscore-dangle": 0
    },
    "env": {
        "jest/globals": true,
        "browser": true,
        "node": true,
        "jasmine": true,
        "worker": true
    }
}